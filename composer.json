{
    "name": "lanffy/laravel-thrift-plugin",
    "description": "thrift rpc server for laravel",
    "type": "library",
    "keywords": [
        "thrift",
        "laravel",
        "rpc"
    ],
    "require": {
        "php": ">=7.1.2",
        "illuminate/http": "5.7.*",
        "illuminate/support": "5.7.*",
        "symfony/http-foundation": "^4.1",
        "apache/thrift": "^0.11.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Lanffy\\Thrift\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests/"
        }
    }
}
